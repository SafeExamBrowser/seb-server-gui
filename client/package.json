{
    "name": "client",
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "sh env.sh && vite",
        "dev-win": "env.sh && vite",
        "build": "vue-tsc --noEmit && vite build",
        "preview": "vite preview",
        "lint:check:file": "eslint",
        "lint:check:all": "eslint .",
        "lint:fix:file": "eslint --fix",
        "lint:fix:all": "eslint --fix .",
        "prettier:check:file": "prettier --check",
        "prettier:check:all": "prettier --check .",
        "prettier:fix:file": "prettier --write",
        "prettier:fix:all": "prettier --write .",
        "format:check:all": "npm run lint:check:all && npm run prettier:check:all",
        "format:fix:all": "npm run lint:fix:all && npm run prettier:fix:all",
        "check:precommit": "lint-staged",
        "quality:fix:all": "npm run lint:fix:all && npm run prettier:fix:all && vue-tsc --noEmit",
        "prepare": "cd .. && husky ./client/.husky"
    },
    "lint-staged": {
        "*.{vue,js,jsx,ts,tsx}": [
            "npm run lint:check:file",
            "npm run prettier:fix:file"
        ],
        "*.{json,md,mdx,css,scss,html,yml,yaml}": "npm run prettier:fix:file"
    },
    "dependencies": {
        "@intlify/unplugin-vue-i18n": "^4.0.0",
        "@mdi/font": "7.4.47",
        "@vuepic/vue-datepicker": "^11.0.1",
        "@vueuse/core": "^10.9.0",
        "axios": "^1.6.8",
        "chart.js": "^4.4.8",
        "core-js": "^3.37.1",
        "date-fns-tz": "^3.1.3",
        "gridstack": "^12.2.2",
        "ipaddr.js": "2.2.0",
        "luxon": "^3.5.0",
        "moment-timezone": "^0.6.0",
        "pinia": "^2.1.7",
        "roboto-fontface": "*",
        "vite-plugin-html": "^3.2.2",
        "vue": "^3.5.11",
        "vue-chartjs": "^5.3.2",
        "vue-i18n": "^9.13.1",
        "vue-loading-overlay": "^6.0.4",
        "vue-router": "^4.4.0",
        "vuetify": "3.10.4",
        "webfontloader": "^1.6.28"
    },
    "devDependencies": {
        "@babel/types": "^7.24.7",
        "@eslint/compat": "^1.3.2",
        "@eslint/js": "^9.35.0",
        "@types/luxon": "^3.4.2",
        "@types/node": "^20.14.10",
        "@types/webfontloader": "^1.6.38",
        "@vitejs/plugin-vue": "^5.0.5",
        "@vue/eslint-config-typescript": "^14.6.0",
        "autoprefixer": "^10.4.19",
        "eslint": "^9.35.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-vue": "^10.4.0",
        "eslint-plugin-vuetify": "^2.5.3",
        "globals": "^16.4.0",
        "husky": "^9.1.7",
        "lint-staged": "^16.1.6",
        "prettier": "^3.6.2",
        "sass": "1.77.6",
        "typescript": "^5.4.5",
        "typescript-eslint": "^8.43.0",
        "unplugin-fonts": "^1.1.1",
        "unplugin-vue-components": "^0.27.2",
        "vite": "^5.3.3",
        "vite-plugin-vuetify": "^2.0.3",
        "vue-tsc": "^2.2.12"
    }
}
