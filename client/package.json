{
    "name": "client",
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "sh env.sh && vite",
        "dev-win": "env.sh && vite",
        "build": "vue-tsc --noEmit && vite build",
        "preview": "vite preview",
        "lint:check:file": "eslint",
        "lint:check:all": "eslint .",
        "lint:fix:file": "eslint --fix",
        "lint:fix:all": "eslint --fix .",
        "prettier:check:file": "prettier --check",
        "prettier:check:all": "prettier --check .",
        "prettier:fix:file": "prettier --write",
        "prettier:fix:all": "prettier --write .",
        "format:check:all": "npm run lint:check:all && npm run prettier:check:all",
        "format:fix:all": "npm run lint:fix:all && npm run prettier:fix:all",
        "check:precommit": "lint-staged",
        "quality:fix:all": "npm run lint:fix:all && npm run prettier:fix:all && vue-tsc --noEmit",
        "prepare": "cd .. && husky ./client/.husky",
        "storybook": "storybook dev -p 6006",
        "typecheck:playwright": "tsc -p tsconfig.playwright.json --noEmit",
        "test:e2e": "npx playwright test",
        "test:e2eui": "npx playwright test --ui",
        "test:e2e:headed": "npx playwright test --headed",
        "test:e2e:interactive": "npx playwright codegen http://localhost:8082",
        "test:e2e:chromium": "npx playwright test --project=chromium --headed",
        "build-storybook": "storybook build"
    },
    "lint-staged": {
        "*.{vue,js,jsx,ts,tsx}": [
            "npm run lint:check:file",
            "npm run prettier:fix:file"
        ],
        "*.{json,md,mdx,css,scss,html,yml,yaml}": "npm run prettier:fix:file"
    },
    "dependencies": {
        "@intlify/unplugin-vue-i18n": "^4.0.0",
        "@mdi/font": "7.4.47",
        "@pinia/testing": "^1.0.2",
        "@vuepic/vue-datepicker": "^11.0.1",
        "@vueuse/core": "^10.9.0",
        "axios": "^1.6.8",
        "chart.js": "^4.4.8",
        "core-js": "^3.37.1",
        "date-fns-tz": "^3.1.3",
        "gridstack": "^12.2.2",
        "ipaddr.js": "2.2.0",
        "luxon": "^3.5.0",
        "moment-timezone": "^0.6.0",
        "roboto-fontface": "*",
        "vite-plugin-html": "^3.2.2",
        "vue": "^3.5.11",
        "vue-chartjs": "^5.3.2",
        "vue-loading-overlay": "^6.0.4",
        "vue-router": "^4.4.0",
        "webfontloader": "^1.6.28",
        "zod": "4.1.12"
    },
    "devDependencies": {
        "@babel/types": "^7.24.7",
        "@chromatic-com/storybook": "^4.1.2",
        "@eslint/compat": "^1.3.2",
        "@eslint/js": "^9.35.0",
        "@playwright/test": "^1.56.1",
        "@storybook/addon-a11y": "^10.0.6",
        "@storybook/addon-docs": "^10.0.4",
        "@storybook/addon-onboarding": "^10.0.4",
        "@storybook/vue3-vite": "^10.0.4",
        "@testing-library/dom": "^10.4.1",
        "@testing-library/vue": "^8.1.0",
        "@types/luxon": "^3.4.2",
        "@types/node": "^20.19.27",
        "@types/webfontloader": "^1.6.38",
        "@vitejs/plugin-vue": "^5.2.4",
        "@vitest/browser-playwright": "^4.0.8",
        "@vitest/coverage-v8": "^4.0.8",
        "@vue/eslint-config-typescript": "^14.6.0",
        "autoprefixer": "^10.4.19",
        "eslint": "^9.35.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-storybook": "^10.0.3",
        "eslint-plugin-vue": "^10.4.0",
        "eslint-plugin-vuetify": "^2.5.3",
        "globals": "^16.4.0",
        "husky": "^9.1.7",
        "jsdom": "^27.1.0",
        "lint-staged": "^16.1.6",
        "pinia": "^3.0.3",
        "playwright": "^1.56.1",
        "prettier": "^3.6.2",
        "sass": "1.77.6",
        "storybook": "^10.0.4",
        "typescript": "^5.4.5",
        "typescript-eslint": "^8.43.0",
        "unplugin-fonts": "^1.1.1",
        "vite": "^6.2.4",
        "vite-plugin-vuetify": "^2.0.3",
        "vitest": "^4.0.8",
        "vue-i18n": "^11.1.12",
        "vue-tsc": "^2.2.12",
        "vuetify": "3.11.0"
    },
    "main": "env.js",
    "keywords": [],
    "author": "",
    "license": "ISC",
    "description": ""
}
